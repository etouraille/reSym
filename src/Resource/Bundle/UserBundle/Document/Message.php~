<?php
namespace Resource\Bundle\UserBundle\Document;

use Doctrine\ORM\Mapping as ORM;
use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;
use JMS\Serializer\Annotation as JMS;
/**
 *  @MongoDB\Document
 */

class Message
{
    /**
     * @MongoDB\String
    */    
    private $from;

   /**
     * @MongoDB\String
    */
    private $to;

    /**
     * @MongoDB\Integer
    */
    private $timestamp;

    /**
     * @MongoDB\String
    */
    private $content;


}
